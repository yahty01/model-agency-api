generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Model {
  id        String   @id @default(uuid())
  name      String
  age       Int
  height    Int
  city      String
  inTown    Boolean
  photoUrl  String
  createdAt DateTime @default(now())
}
